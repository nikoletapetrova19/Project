package sk.itsovy.petrova.project;

public interface ArrayMethods {

    public double getAverageValue();
    public int min();
    public int max();
    public int min2();  /*volatile */
    public int max2();  /*volatile*/
    public void generateValues(int a, int b);  //hranice najv. a najm.
    public boolean contains(int value);
    public int countOfValues(int value);
    public int countOfEvenValues();
    public void incrementValues();  //cele pole zvacsi o 1
    public void sort(boolean asc);
    public void addItem(int newValue);  //prida na koniec pola novu polozku
    public void addItem(int newValue, int position);
    public int[]copy();
    //public String toString();
    public int getItem(int position);



